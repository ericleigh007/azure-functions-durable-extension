{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "durableTask": {
        "tracing": {
            "DistributedTracingEnabled": true,
            "Version": "V2"
        },
        "defaultVersion": "2.0",
        "versionMatchStrategy": "CurrentOrOlder",
        "versionFailureStrategy": "Fail"
    }
  }
}